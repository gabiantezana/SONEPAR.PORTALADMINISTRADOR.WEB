@model IPagedList<SICER.MODEL.SapConcepto>
<table class="table table-hover">
    <thead style="font-weight:bold">
        <tr>
            <td>Code</td>
            <td>Name</td>
            <td>U_MSS_ACCT</td>
            <td>U_MSS_DSCRPT</td>
        </tr>
    </thead>
    <tbody>
        @if (!Model.Any())
        {
            <tr><td style="font-style: italic" colspan="7">@ConstantHelper.MENSAJE_TABLA_VACIA</td></tr>
        }
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Code</td>
                <td>@item.Name</td>
                <td>@item.U_MSS_ACC</td>
                <td>@item.U_MSS_DSC</td>
            </tr>
        }
    </tbody>
</table>
<div id="myPager">
    @Html.PagedListPager(Model, page => Url.Action("_PagedList",
        new
        {
            page = page,
            //MODEL PROPERTIES HERE:
            filter = ViewBag.Filter,
        }), PagedListRenderOptions.EnableUnobtrusiveAjaxReplacing("partialList"))
</div>
