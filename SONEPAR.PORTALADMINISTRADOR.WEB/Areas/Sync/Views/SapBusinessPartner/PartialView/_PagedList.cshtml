@model IPagedList<SICER.MODEL.SapBusinessPartner>
<table class="table table-hover">
    <thead style="font-weight:bold">
        <tr>
            <td>CardCode</td>
            <td>CardName</td>
            <td>LictradNum</td>
            <td>CardType</td>
            <td>validFor</td>
        </tr>
    </thead>
    <tbody>
        @if (!Model.Any())
        {
            <tr><td style="font-style: italic" colspan="7">@ConstantHelper.MENSAJE_TABLA_VACIA</td></tr>
        }
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.CardCode</td>
                <td>@item.CardName</td>
                <td>@item.LictradNum</td>
                <td>@item.CardType</td>
                <td>@item.validFor</td>
            </tr>
        }
    </tbody>
</table>
<div id="myPager">
    @Html.PagedListPager(Model, page => Url.Action("_PagedList",
        new
        {
            page = page,
            //MODEL PROPERTIES HERE:
            filter = ViewBag.Filter,
        }), PagedListRenderOptions.EnableUnobtrusiveAjaxReplacing("partialList"))
</div>
